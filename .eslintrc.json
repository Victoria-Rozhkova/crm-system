{
  "root": true,
  "env": {
    "node": true
  },
  "extends": ["plugin:vue/vue3-essential", "eslint:recommended"],
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "rules": {
    "no-unused-vars": "off",
    "no-empty-pattern": "off",
    "no-empty": "off",
    "vue/multi-word-component-names": "off"
  }
}
